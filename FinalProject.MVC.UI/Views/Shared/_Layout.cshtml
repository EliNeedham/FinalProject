<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta content="width=device-width, initial-scale=1.0" name="viewport">

    <title>@ViewBag.Title | Maid in KC</title>
    <meta content="" name="descriptison">
    <meta content="" name="keywords">
    <link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.22/css/jquery.dataTables.css">
    <link href="https://cdn.datatables.net/responsive/2.2.1/css/responsive.dataTables.min.css" rel="stylesheet" />

    <!-- Favicons -->
    <link href="~/Content/assets/img/favicon-32x32.png" rel="icon">
    <link href="~/Content/assets/img/apple-icon-180x180.png" rel="apple-touch-icon">

    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i|Roboto:300,300i,400,400i,500,500i,600,600i,700,700i|Poppins:300,300i,400,400i,500,500i,600,600i,700,700i" rel="stylesheet">

    @Styles.Render("~/Content/css")

    <!-- =======================================================
    * Template Name: Company - v2.1.0
    * Template URL: https://bootstrapmade.com/company-free-html-bootstrap-template/
    * Author: BootstrapMade.com
    * License: https://bootstrapmade.com/license/
    ======================================================== -->
</head>

<body>

    <!-- ======= Header ======= -->
    <header id="header" class="fixed-top">
        <div class="container d-flex align-items-center">

            @*<h1 class="logo mr-auto"><a href="index.html"><span>Com</span>pany</a></h1>*@
            <!-- Uncomment below if you prefer to use an image logo -->
            <a href="@Url.Action("Index","Home")" class="logo mr-auto"><img src="~/Content/assets/img/logo.png" alt="" class="img-fluid"></a>

            <nav class="nav-menu d-none d-lg-block">
                <ul>
                    <li class="active"><a href="@Url.Action("Index", "Home")">Home</a></li>

                    <li class="drop-down">
                        <a href="@Url.Action("About", "Home")">About</a>
                        <ul>
                            <li><a href="@Url.Action("About", "Home")">About Us</a></li>
                            <li><a href="@Url.Action("Index", "Services")">Services</a></li>
                            <li><a href="@Url.Action("Index", "Locations")">Locations</a></li>
                        </ul>
                    </li>
                    @if (User.IsInRole("Admin") || User.IsInRole("Employee"))
                    {
                        <li class="drop-down">
                            <a href="#">Admin | Staff</a>
                            <ul>
                                @if (User.IsInRole("Admin"))
                                {
                                    <li><a href="@Url.Action("Index", "RolesAdmin")">Roles Admin</a></li>
                                    <li><a href="@Url.Action("Index", "UsersAdmin")">Users Admin</a></li>

                                }
                                <li><a href="@Url.Action("Index", "UserDetails")">User Details</a></li>
                                <li><a href="@Url.Action("Index", "Homes")">Homes</a></li>
                                <li><a href="@Url.Action("Index", "Reservations")">Reservations</a></li>
                                <li><a href="@Url.Action("Index", "Services")">Services</a></li>
                                <li><a href="@Url.Action("Index", "Locations")">Locations</a></li>
                            </ul>
                        </li>
                    }
                    @if (User.IsInRole("Client"))
                    {
                        <li class="drop-down">
                            <a href="#">My Account</a>
                            <ul>
                                <li><a href="@Url.Action("Index", "UserDetails")">Personal Details</a></li>
                                <li><a href="@Url.Action("Index", "Homes")">Homes</a></li>
                                <li><a href="@Url.Action("Index", "Reservations")">Reservations</a></li>
                                <li><a href="@Url.Action("Index", "Services")">Services</a></li>
                                <li><a href="@Url.Action("Index", "Locations")">Locations</a></li>
                            </ul>
                        </li>
                    }

                    <li><a href="@Url.Action("Contact","Home")">Contact</a></li>
                    <li>@Html.Partial("_LoginPartial")</li>

                </ul>
            </nav><!-- .nav-menu -->

            <div class="header-social-links">
                <a href="https://www.linkedin.com/in/elizabeth-needham-726155103/" class="linkedin"><i class="icofont-linkedin"></i></a>
            </div>

        </div>
    </header><!-- End Header -->
    <div class="clearfix"></div>
    <main id="main">
        @RenderBody()
    </main>



    <!-- ======= Footer ======= -->
    <footer id="header">
        <div class="footer-top">
            <div class="container">
                <a href="@Url.Action("Index","Home")" class="logo mr-auto"><img src="~/Content/assets/img/logo.png" alt="" class="img-fluid"></a>
                <div class="row">

                    <div class="col-lg-4 col-md-4 footer-contact">
                        <p>
                            Brookside<br>
                            Kansas City Missouri<br>
                            United States
                        </p>
                    </div>
                    <div class="col-lg-3 offset-lg-5 col-md-4 footer-contact">
                        <div >
                            <strong>Phone:</strong> +1 123 456 7899<br>
                            <strong>Email:</strong> EliNeedham@Outlook.com<br>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        @*<div class="container d-md-flex py-4">*@
        <div class="mr-md-auto text-center">
            <div class="copyright">
                &copy; Copyright <strong><span>Elizabeth Needham</span></strong>. All Rights Reserved
            </div>
            <div class="credits">
                <!-- All the links in the footer should remain intact. -->
                <!-- You can delete the links only if you purchased the pro version. -->
                <!-- Licensing information: https://bootstrapmade.com/license/ -->
                <!-- Purchase the pro version with working PHP/AJAX contact form: https://bootstrapmade.com/company-free-html-bootstrap-template/ -->
                Designed by <a href="https://bootstrapmade.com/">BootstrapMade</a> ||
                Adapted by Elizabeth Needham
            </div>
        </div>
        @*</div>*@
    </footer><!-- End Footer -->

    <a href="#" class="back-to-top"><i class="icofont-simple-up"></i></a>

    @Scripts.Render("~/bundles/jquery")

    <script type="text/javascript" charset="utf8" src="https://cdn.datatables.net/1.10.22/js/jquery.dataTables.js"></script>
    <script src="https://cdn.datatables.net/responsive/2.2.1/js/dataTables.responsive.min.js"></script>
    <script src="~/Content/assets/js/Custom.js"></script>

    @RenderSection("scripts", required: false)

</body>

</html>